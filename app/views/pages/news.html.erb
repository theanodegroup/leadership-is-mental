<% partial_path = "pages/#{@page_name}" %>

<% content_for :page_header, @title %>
<% breadcrumb @breadcrumb %>


<% @aricles = @leadership_jobs.page(params[:page]).per(5) %>
<% @aricles.each do |article| %>
  <%= render partial: 'shared/article', locals: { article: article } %>
<% end %>


<%= paginate @leadership_jobs %>