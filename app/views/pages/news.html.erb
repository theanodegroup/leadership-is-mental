<% partial_path = "pages/#{@page_name}" %>

<% content_for :page_header, @title %>
<% breadcrumb @breadcrumb %>


<% @leadership_articles = @leadership_articles.reorder(pub_date: :desc).page(params[:page]).per(5) %>
<% @leadership_articles.each do |article| %>
  <%= render partial: 'shared/article', locals: { article: article } %>
<% end %>


<%= paginate @leadership_articles %>