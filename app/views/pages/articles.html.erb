<% partial_path = "pages/#{@page_name}" %>

<% content_for :page_header, @title %>
<% breadcrumb @breadcrumb %>


<% @articles = @articles.page(params[:page]).per(5) %>
<% @articles.each do |article| %>
  <%= render partial: 'shared/article', locals: { article: article } %>
<% end %>

<%= paginate @articles %>